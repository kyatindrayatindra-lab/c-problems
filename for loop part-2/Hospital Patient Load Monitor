#include <stdio.h>

int main() {
int N, patients, total = 0, overcrowded = 0;

scanf("%d", &N);

for(int i = 0; i < N; i++) {
scanf("%d", &patients);
total += patients;
if(patients > 100)
overcrowded++;
}
printf("Total Patients: %d\n", total);
printf("Overcrowded Days: %d\n", overcrowded);

return 0;
}
