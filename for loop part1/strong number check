#include <stdio.h>

int main() {
    int N, temp, digit, sum = 0;

    scanf("%d", &N);
    temp = N;

    while (temp > 0) {
        digit = temp % 10;

        int fact = 1;
        for (int i = 1; i <= digit; i++) {
            fact *= i;
        }

        sum += fact;
        temp /= 10;
    }

    if (sum == N) {
        printf("Yes");
    } else {
        printf("No");
    }

    return 0;
}
