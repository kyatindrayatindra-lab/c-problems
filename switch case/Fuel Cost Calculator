#include <stdio.h>

int main() {
int fuelType, litres;
int cost = 0;

   
scanf("%d", &fuelType);
scanf("%d", &litres);

switch(fuelType) {
case 1: 
cost = litres * 105;
break;

case 2: 
cost = litres * 92;
break;

case 3: 
cost = litres * 85;
break;

default:
printf("Invalid fuel type");
return 0;
}

printf("Fuel Cost â‚¹%d", cost);

return 0;
}
