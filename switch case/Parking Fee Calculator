#include <stdio.h>

int main() {
    int vehicleType, hours;
    int rate, totalFee;

    scanf("%d", &vehicleType);
    scanf("%d", &hours);

    switch(vehicleType) {
        case 1:
            rate = 10;   
            break;
        case 2:
            rate = 20;   
            break;
        default:
            rate = 20;   
    }

    
    totalFee = rate * hours;

  
    printf("Parking Fee â‚¹%d", totalFee);

    return 0;
}
