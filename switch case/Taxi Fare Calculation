#include <stdio.h>

int main() {
int vehicleType, distance;
int rate = 0, fare;

scanf("%d", &vehicleType);
scanf("%d", &distance);

switch(vehicleType) {
case 1: 
rate = 10;
break;

case 2: 
rate = 15;
break;

case 3:  
rate = 20;
break;

default:
rate = 0;  
}

fare = rate * distance;


printf("â‚¹%d\n", fare);

return 0;
}
